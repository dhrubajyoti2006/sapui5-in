<mvc:View
	controllerName="sapui5in.ui.controller.App"
	displayBlock="true"
	xmlns:f="sap.f"
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
	xmlns:tnt="sap.tnt"
	xmlns:mvc="sap.ui.core.mvc">

	<tnt:ToolPage>
		<tnt:header>
			<f:ShellBar title="Master Detail with Routing"
									secondTitle="Boilerplate application"
									showMenuButton="true"
									showCopilot="true"
									showNotifications="true"
									menuButtonPressed="onMenuButtonPress"
									notificationsNumber="2">
				<f:menu>
					<Menu>
						<MenuItem text="Flight booking" icon="sap-icon://flight" />
						<MenuItem text="Car rental" icon="sap-icon://car-rental" />
					</Menu>
				</f:menu>
				<f:profile>
					<Avatar initials="UI"/>
				</f:profile>
				<f:searchManager>
					<f:SearchManager/>
				</f:searchManager>
			</f:ShellBar>
		</tnt:header>
		<tnt:mainContents>
			<App id="app" />
		</tnt:mainContents>
	</tnt:ToolPage>

</mvc:View>
